$hyoo_sketch_editor_paper $mol_view
	grid NaN
	event *
		^
		pointerdown? <=> pointer_down? null
	style *
		^
		width <= width NaN
		height <= height NaN
		backgroundSize <= background_size \
	attr *
		^
		hyoo_sketch_editor_paper_preview <= preview false
		hyoo_sketch_editor_paper_focused <= selected? false
