$hyoo_sketch_project_list $mol_page
	person $hyoo_sketch_person
		domain => domain
	title @ \Projects
	project_name_default @ \New project
	tools /
		<= Source $mol_link_source
			uri \https://github.com/hyoo-ru/sketch.hyoo.ru
		<= Settings_link $mol_link
			arg * settings \
			hint @ \Open Settings
			sub /
				<= Settings_icon $mol_icon_settings
		<= Lights $mol_lights_toggle
		<= Project_add $mol_button_minor
			hint @ \Add project
			click? <=> project_add? null
			sub / <= Project_add_icon $mol_icon_plus
	body /
		<= List $mol_list
			rows <= projects /
	Project* $mol_link
		arg *
			project <= project_id* \
			page null
			selected null
			preview null
		title <= project_name* \
