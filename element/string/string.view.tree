$hyoo_sketch_element_string $hyoo_sketch_element
	element $hyoo_sketch_element_string_model
		hint? => hint?
		value? => value?
	Element <= Content $mol_string
		hint <= hint
		value <= value
	options /
		^
		<= Hint_field $mol_form_field
			name @ \Placeholder
			control <= Hint_control $mol_string
				value? <=> hint?
		<= Value_field $mol_form_field
			name @ \Value
			control <= Value_control $mol_string
				value? <=> value?
